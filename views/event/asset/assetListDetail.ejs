<div class="accordion-detail">
  <!-- accordion-detail-content -->
  <div class="accordion-detail-content">
    <div class="table-comp">
      <table class="table detail-style">
        <caption><%= __("eventAsset|eventAssetDetailInformation") %></caption>
          <colgroup>
            <col style="width: 25%; " />
            <col style="width: 25%; " />
            <col style="width: 25%; " />
            <col style="width: 25%; " />
          </colgroup>
        <tbody>
          <% for (let i = 0; i < ObjView.length; i++) { %>
          <% const data = ObjView[i] %>
          <tr>
            <td>
              <div class="detail-info">
                <dl>
                  <dt><%= __("eventAsset|Asset IP") %></dt>
                  <dd><%= data.asset_ip %></dd>
                </dl>
              </div>
            </td>
            <td>
              <div class="detail-info">
                <dl>
                  <dt><%= __("eventAsset|Asset type") %></dt>
                  <dd><%= data.asset_type %></dd>
                </dl>
              </div>
            </td>
            <td>
              <div class="detail-info">
                <dl>
                  <dt><%= __("eventAsset|Asset MAC") %></dt>
                  <dd><%= data.asset_mac %></dd>
                </dl>
              </div>
            </td>
            <td>
              <div class="detail-info">
                <dl>
                  <dt><%= __("eventAsset|Asset manufacturer") %></dt>
                  <dd><%= data.asset_manufacturer %></dd>
                </dl>
              </div>
            </td>
          </tr>
          <tr>
            <td colspan="4">
              <div class="detail-info">
                <dl>
                  <dt><%= __("eventAsset|Response") %></dt>
                  <dd><%= data.response %></dd>
                </dl>
              </div>
            </td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </div>
  <!-- //accordion-detail-content -->
</div>

<script>
  logdata('/event/asset/assetdetail', <%- JSON.stringify(locals) %>);
</script>
