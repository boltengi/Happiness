<div class="modal-comp longer-size" id="modalBluetoothDetail" data-height="864px">
  <div class="modal-align">
    <div class="modal-box">
      <div class="modal-wrap modal-tab">
        <!-- Modal Header -->
        <div class="modal-header">
          <div class="modal-title">
            <div class="modal-tab-links">
              <div class="modal-tab-link on"><a href=""><%= __("wirelessRadar|Bluetooth") %></a></div>
            </div>
          </div>
          <button type="button" class="modal-btn-close"><span class="sr-only"><%= __("wirelessRadar|close") %></span></button>
        </div>
        <!-- Modal container -->
        <div class="modal-container">
          <div class="modal-tab-content on">
            <div class="modal-info-wrap">

              <% for (let i = 0; i < ObjView.length; i++) { %>
              <% const data = ObjView[i] %>
              <div class="info-group-wrap">
                <!-- info-group-row -->
                <div class="info-group-row">
                  <ul>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Severity") %></dt>
                          <dd>
                            <% if ( data.severity == 0 ){ %> <%= __("wirelessRadar|LOW") %>
                            <% } else if ( data.severity == 1 ){ %> <%= __("wirelessRadar|MID") %>
                            <% } else if ( data.severity == 2 ){ %> <%= __("wirelessRadar|HIGH") %>
                            <% } %>
                          </dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Asset name") %></dt>
                          <dd><%= data.asset_name %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Mode") %></dt>
                          <dd><%= data.mode %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|OUI") %></dt>
                          <dd><%= data.oui %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|TX Power") %></dt>
                          <dd><%= data.tx_power %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Processed time") %></dt>
                          <dd><%= dateFormat(data.detection_time) %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group<% if(data.uuid) { %> item-style <% } %>">
                        <dl>
                          <dt><%= __("wirelessRadar|UUIDs") %></dt>
                          <dd>
                            <ul class="ul-list top-spacing">
                              <% if(data.uuid) { %>
                                <% const arr = data.uuid.split(';') %>
                                <% for(let j = 0; j < arr.length; j++) { %>
                                <% const data = arr[j] %>
                                <% if (j < arr.length - 1){ %>
                                <li class="list-style"><%= data %></li>
                                <% } %>
                                <% } %>
                              <% } %>
            								</ul>
                          </dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Major class") %></dt>
                          <dd><%= data.major_class %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group<% if(data.service_class) { %> item-style <% } %>">
                        <dl>
                          <dt><%= __("wirelessRadar|Sevice class") %></dt>
                          <dd>
                            <ul class="ul-list">
                              <% if(data.service_class) { %>
                                <% const arr = data.service_class.split(';') %>
                                <% for(let j = 0; j < arr.length; j++) { %>
                                  <% const data = arr[j] %>
                                  <% if (j < arr.length - 1){ %>
                                  <li class="list-style"><%= data %></li>
                                  <% } %>
                                <% } %>
                              <% } %>
            								</ul>
                          </dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Flags") %></dt>
                          <dd>
                            <%= data.flags %>
                            <ul class="ul-list top-spacing">
                              <% if(data.flag_list) { %>
                                <% const arr = data.flag_list.split(';') %>
                                <% for(let j = 0; j < arr.length; j++) { %>
                                <% const data = arr[j] %>
                                <% if (j < arr.length - 1){ %>
                                <li class="list-style"><%= data %></li>
                                <% } %>
                                <% } %>
                              <% } %>
            								</ul>
                          </dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                  </ul>
                </div>
                <!-- //info-group-row -->
                <!-- info-group-row -->
                <div class="info-group-row">
                  <ul>
                    <li>
                      <!-- info-group -->
                      <div class="info-group<% if(data.severity_info) { %> item-style <% } %>">
                        <dl>
                          <dt><%= __("wirelessRadar|Severity info") %></dt>
                          <dd>
                            <ul class="ul-list">
                              <% if(data.severity_info) { %>
                                <% const arr = data.severity_info.split(',') %>
                                <% for(let j = 0; j < arr.length; j++) { %>
                                <% const data = arr[j] %>
                                <li class="list-style"><%= data %></li>
                                <% } %>
                              <% } %>
            								</ul>
                          </dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Asset type") %></dt>
                          <dd><%= data.asset_type %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Asset MAC") %></dt>
                          <dd><%= data.asset_mac %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Address type") %></dt>
                          <dd><%= data.address_type %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|RSSI") %></dt>
                          <dd><%= data.rssi %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Manufacturer") %></dt>
                          <dd><%= data.manufacturer %>(<%= data.manufacturer_id %>)</dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group<% if(data.service_data) { %> item-style <% } %>">
                        <dl>
                          <dt><%= __("wirelessRadar|Sevice data") %></dt>
                          <dd>
                            <% if(data.service_data) { %>
                              <% const arr = data.service_data.split(';') %>
                              <% for(let j = 0; j < arr.length; j++) { %>
                                <% const data = arr[j] %>
                                <% if (j < arr.length - 1){ %>
                                <li class="list-style"><%= data %></li>
                                <% } %>
                              <% } %>
                            <% } %>
                          </dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Class") %></dt>
                          <dd>
                            <%= data.class %> <br>
                            <% if (data.cod_changed == 1) { %><span class="warning-color"><%= __("wirelessRadar|(The device type cannot be changed.") %><br><%= __("wirelessRadar|Warning : MITM attack.)") %></span><% } %>
                          </dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Minor class") %></dt>
                          <dd><%= data.minor_class %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                    <li>
                      <!-- info-group -->
                      <div class="info-group">
                        <dl>
                          <dt><%= __("wirelessRadar|Modalias") %></dt>
                          <dd><%= data.modalias %></dd>
                        </dl>
                      </div>
                      <!-- //info-group -->
                    </li>
                  </ul>
                </div>
                <!-- //info-group-row -->
              </div>
              <% } %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script>
  logdata('/wireless/radarDetection/bluetoothDetail', <%- JSON.stringify(locals) %>);
</script>
<script>
  var modalBluetoothDetail = popupModal('#modalBluetoothDetail');
  modalBluetoothDetail.open('#modalBluetoothDetail');
</script>
